{
    "elmType": "div",
    "style": {
        "flex-wrap": "wrap",
        "display": "flex"
    },
    "children": [{
        "forEach": "__INTERNAL__ in @currentField",
        "elmType": "div",
        "style": {
            "box-sizing": "border-box",
            "padding": "4px 8px 5px 8px",
            "display": "flex",
            "border-radius": "16px",
            "height": "24px",
            "align-items": "center",
            "white-space": "nowrap",
            "overflow": "hidden",
            "margin": "1px 4px 4px 0px"
        },
        "attributes": {
            "class": {
                "operator": ":",
                "operands": [{
                        "operator": "==",
                        "operands": [
                            "[$__INTERNAL__]",
                            ""
                        ]
                    },
                    "",
                    {
                        "operator": ":",
                        "operands": [{
                                "operator": "==",
                                "operands": [
                                    "[$__INTERNAL__]",
                                    "Planning"
                                ]
                            },
                            "sp-css-backgroundColor-neutralBackground sp-field-borderBottomRegular sp-field-borderBottomSolid sp-css-borderColor-neutralTertiary",
                            {
                                "operator": ":",
                                "operands": [{
                                        "operator": "==",
                                        "operands": [
                                            "[$__INTERNAL__]",
                                            "Design"
                                        ]
                                    },
                                    "sp-css-backgroundColor-blueBackground07 sp-field-borderBottomRegular sp-field-borderBottomSolid sp-css-borderColor-blueDark",
                                    {
                                        "operator": ":",
                                        "operands": [{
                                                "operator": "==",
                                                "operands": [
                                                    "[$__INTERNAL__]",
                                                    "Engineering"
                                                ]
                                            },
                                            "sp-css-backgroundColor-successBackground sp-field-borderBottomRegular sp-field-borderBottomSolid sp-css-borderColor-green",
                                            {
                                                "operator": ":",
                                                "operands": [{
                                                        "operator": "==",
                                                        "operands": [
                                                            "[$__INTERNAL__]",
                                                            "Marketing"
                                                        ]
                                                    },
                                                    "sp-css-backgroundColor-warningBackground sp-field-borderBottomRegular sp-field-borderBottomSolid sp-css-borderColor-neutralTertiary",
                                                    {
                                                        "operator": ":",
                                                        "operands": [{
                                                                "operator": "==",
                                                                "operands": [
                                                                    "[$__INTERNAL__]",
                                                                    "Research"
                                                                ]
                                                            },
                                                            "sp-css-backgroundColor-errorBackground sp-field-borderBottomRegular sp-field-borderBottomSolid sp-css-borderColor-themeDarkAlt",
                                                            ""
                                                        ]
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        },
        "children": [{
                "elmType": "span",
                "style": {
                    "line-height": "16px",
                    "height": "14px"
                },
                "attributes": {
                    "iconName": {
                        "operator": ":",
                        "operands": [{
                                "operator": "==",
                                "operands": [
                                    "[$__INTERNAL__]",
                                    ""
                                ]
                            },
                            "",
                            {
                                "operator": ":",
                                "operands": [{
                                        "operator": "==",
                                        "operands": [
                                            "[$__INTERNAL__]",
                                            "Planning"
                                        ]
                                    },
                                    "Calendar",
                                    {
                                        "operator": ":",
                                        "operands": [{
                                                "operator": "==",
                                                "operands": [
                                                    "[$__INTERNAL__]",
                                                    "Design"
                                                ]
                                            },
                                            "Like",
                                            {
                                                "operator": ":",
                                                "operands": [{
                                                        "operator": "==",
                                                        "operands": [
                                                            "[$__INTERNAL__]",
                                                            "Engineering"
                                                        ]
                                                    },
                                                    "Trending12",
                                                    {
                                                        "operator": ":",
                                                        "operands": [{
                                                                "operator": "==",
                                                                "operands": [
                                                                    "[$__INTERNAL__]",
                                                                    "Marketing"
                                                                ]
                                                            },
                                                            "Mail",
                                                            {
                                                                "operator": ":",
                                                                "operands": [{
                                                                        "operator": "==",
                                                                        "operands": [
                                                                            "[$__INTERNAL__]",
                                                                            "Research"
                                                                        ]
                                                                    },
                                                                    "People",
                                                                    ""
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            {
                "elmType": "span",
                "style": {
                    "overflow": "hidden",
                    "text-overflow": "ellipsis",
                    "padding": "0 3px"
                },
                "txtContent": "[$__INTERNAL__]"
            }
        ]
    }]
}