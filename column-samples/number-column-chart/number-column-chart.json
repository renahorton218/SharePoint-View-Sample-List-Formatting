{
  "$schema": "https://developer.microsoft.com/json-schemas/sp/column-formatting.schema.json",
  "elmType": "div",
  "style": {
    "fill": "currentColor",
    "height": "104px"
  },
  "attributes": {
    "class": "ms-fontColor-themePrimary"
  },
  "children": [
    {
      "elmType": "div",
      "style": {
        "height": "100%"
      },
      "attributes": {
        "title": "[$January]"
      },
      "children": [
        {
          "elmType": "svg",
          "style": {
            "width": "20px",
            "height": "100px",
            "padding": "0 4px"
          },
          "children": [
            {
              "elmType": "path",
              "attributes": {
                "d": "= 'M 0,100 L 0,' + (100 - [$January]) + ' 20,' + (100 - [$January]) + ' 20,100'"
              }
            }
          ]
        }
